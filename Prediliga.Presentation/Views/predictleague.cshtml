<div class="container">
    <div class="row clearfix">
        <div class="col-md-6 column">
            <h1>
                (NombreLiga) : (IDLiga)
            </h1>
        </div>
        <div class="col-md-6 column">
            <img alt="140x140" src="http://lorempixel.com/140/140/" class="img-circle" />
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-md-12 column">
            <h2>
                Partido (IDPartido)
            </h2>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-md-4 column">
            <h3>
                (Equipo1)
            </h3>
        </div>
        <div class="col-md-4 column">
            <h3>
                VS.
            </h3>
        </div>
        <div class="col-md-4 column">
            <h3>
                (Equipo2)
            </h3>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-md-4 column">
            <input ng-model="PredictE1" type="text" class="form-control" placeholder="Escribe tu resultado predecido"/>
        </div>
        <div class="col-md-4 column">
        </div>
        <div class="col-md-4 column">
            <input ng-model="PredictE2" type="text" class="form-control" placeholder="Escribe tu resultado predecido" />
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-md-6 column">
            <button type="button" class="btn btn-danger btn-lg btn-block">Predict</button>
        </div>
        <div class="col-md-6 column">
            <button type="button" class="btn btn-lg btn-block btn-primary">Cancel</button>
        </div>
    </div>
</div>